{
  "name": "@yz13/blog",
  "version": "0.0.0",
  "private": true,
  "description": "Blog package for yz13",
  "keywords": [
    "blog",
    "package"
  ],
  "type": "module",
  "scripts": {
    "dev": "content-collections watch",
    "build": "content-collections build",
    "generate": "bun run scripts/get-posts.ts && bun run scripts/get-drafts.ts",
    "generate:posts": "bun run scripts/get-posts.ts",
    "generate:drafts": "bun run scripts/get-drafts.ts",
    "generate:new": "bun run scripts/new-draft.ts",
    "upload": "bun run scripts/upload-posts.ts && bun run scripts/upload-drafts.ts",
    "upload:posts": "bun run scripts/upload-posts.ts",
    "upload:drafts": "bun run scripts/upload-drafts.ts"
  },
  "devDependencies": {
    "@content-collections/cli": "^0.1.7",
    "@content-collections/core": "^0.12.0",
    "@content-collections/next": "^0.2.9",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@yz13/eslint-config": "*",
    "@yz13/typescript-config": "*",
    "eslint": "^9.38.0",
    "typescript": "5.9.2",
    "zod": "^4.1.12"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  }
}
