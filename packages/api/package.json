{
  "name": "@yz13/api",
  "version": "0.0.0",
  "description": "Package with rest-api endpoints for current monorepo",
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./lib/make-user-obj": {
      "types": "./dist/lib/make-user-obj.d.ts",
      "import": "./dist/lib/make-user-obj.mjs",
      "require": "./dist/lib/make-user-obj.js"
    },
    "./lib/is-admin": {
      "types": "./dist/lib/is-admin.d.ts",
      "import": "./dist/lib/is-admin.mjs",
      "require": "./dist/lib/is-admin.js"
    },
    "./lib/filters": {
      "types": "./dist/lib/filters.d.ts",
      "import": "./dist/lib/filters.mjs",
      "require": "./dist/lib/filters.js"
    },
    "./lib/avatar-url": {
      "types": "./dist/lib/avatar-url.d.ts",
      "import": "./dist/lib/avatar-url.mjs",
      "require": "./dist/lib/avatar-url.js"
    },
    "./types/timezone": "./dist/types/timezone.d.ts",
    "./types/articles": "./dist/types/articles.d.ts",
    "./types/pricing": "./dist/types/pricing.d.ts",
    "./types/user": "./dist/types/user.d.ts",
    "./types/attachments": "./dist/types/attachments.d.ts",
    "./types/rooms": "./dist/types/charts.d.ts",
    "./types/store": "./dist/types/store.d.ts",
    "./types/calendar": "./dist/types/calendar.d.ts",
    "./types/positions": "./dist/types/positions.d.ts",
    "./lib/custom-fetch": {
      "types": "./dist/const/fetch.d.ts",
      "import": "./dist/const/fetch.mjs",
      "require": "./dist/const/fetch.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "publish": "bun run build",
    "build": "tsup",
    "dev": "tsup --watch --clean=false"
  },
  "keywords": [
    "package",
    "rest-api"
  ],
  "author": "YZ13-ENV",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "typescript": "^5.8.3",
    "@yz13/supabase": "*"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "next": "^15.3.0",
    "tsup": "^6.7.0",
    "typescript-config": "*"
  },
  "engines": {
    "node": ">=20"
  }
}
